{
  "name": "unsourcemap",
  "version": "0.9.0",
  "author": "Tim McCormack <cortex@brainonfire.net>",
  "description": "Unpack a minified JS file using a source map",
  "repository": {
    "type": "git",
    "url": "https://github.com/timmc/unsourcemap.git"
  },
  "scripts": {
    "run": "tsc --project tsconfig.json ; rm -rf out/; node unsourcemap.js main.prod.js main.prod.js.map out",
    "compile": "tsc --project tsconfig.json"
  },
  "dependencies": {
    "argparse": "^1.0.10",
    "source-map": "^0.7.3"
  },
  "devDependencies": {
    "@types/argparse": "^1.0.36",
    "@types/node": "^12.0.8",
    "typescript": "^3.3.1"
  },
